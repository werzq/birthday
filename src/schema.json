{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "people": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "birthday": {
              "type": "string",
              "format": "date"
            }
          },
          "required": ["name", "birthday"]
        }
      }
    },
    "required": ["people"]
}
  